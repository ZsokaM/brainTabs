<h1 class="main-header">My dashboard</h1>

<section class="container">
  <section class="side side-left">
    <a href="/newfolder"><img src="Images/add.png" width="30px" /></a>

    <section class="card-layout">
      {{#each user.folders}}
        <article class='card'>
              <h4>{{this.title}}</h4>
              <div class="card-links">
                <a href="/profile/{{this._id}}"><img src="Images/book.png" width="30px"></a>
                <form action="/profile/{{this._id}}/delete" method="POST">
                  <button type="submit" style="border: 0; widht: 30px;background: transparent"><img src="Images/delete.png" width="30px"></button>
                </form>
              </div>
        </article>
      {{/each}}
    </section>
 
  
</div>

  </section>
  <section class="side side-right">
    <a href="#"><img src="Images/add.png" width="30px" disabled/></a>
    <p>New features coming soon</p>
  </section>
</section>